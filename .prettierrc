{
  "$schema": "http://json.schemastore.org/prettierrc",
  "endOfLine": "lf",
  "printWidth": 120,
  "quoteProps": "as-needed",
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "none",
  "useTabs": false,
  "overrides": [
    {
      "files": "src/assets/*.{ts,json}",
      "options": {
        "printWidth": 200
      }
    }
  ]
}
