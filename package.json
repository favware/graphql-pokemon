{
  "name": "@favware/graphql-pokemon",
  "version": "1.8.0",
  "description": "Expansive Pokemon GraphQL API",
  "author": "@favware",
  "license": "MIT",
  "types": "dist/graphql-pokemon.d.ts",
  "main": "dist/graphql-pokemon.js",
  "homepage": "https://favware.tech/api",
  "scripts": {
    "lint": "eslint src scripts __tests__ --ext ts --fix",
    "style": "prettier --write --loglevel=warn src/**/*.{ts,json}",
    "test": "jest",
    "build": "tsc -b src",
    "watch": "tsc -b src -w",
    "dev": "nodemon build/index.js",
    "commit": "git-cz",
    "cz": "git-cz",
    "up": "yarn upgrade-interactive --latest",
    "sversion": "standard-version",
    "codegen": "graphql-codegen && tsc -b typings",
    "cloud:start": "node index.js",
    "prepublishOnly": "yarn codegen",
    "scripts:runner": "ts-node -P tsconfig.eslint.json",
    "scripts:validateNoOnly": "yarn scripts:runner scripts/validateNoOnly",
    "scripts:yarnAfterPull": "yarn scripts:runner scripts/yarnAfterPull",
    "scripts:smogontiersupdater": "yarn scripts:runner scripts/updateSmogonTiers",
    "scripts:updatelearnsets": "yarn scripts:runner scripts/UpdateLearnsets"
  },
  "dependencies": {
    "graphql": "^14.6.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@discordjs/collection": "^0.1.5",
    "@favware/eslint-config": "^3.3.1",
    "@favware/ts-config": "^1.1.3",
    "@graphql-codegen/cli": "^1.13.2",
    "@graphql-codegen/typescript": "^1.13.2",
    "@graphql-codegen/typescript-resolvers": "^1.13.2",
    "@klasa/timestamp": "^0.0.1",
    "@types/express": "^4.17.6",
    "@types/glob": "^7.1.1",
    "@types/graphql": "^14.5.0",
    "@types/graphql-type-json": "^0.3.2",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.0",
    "@types/node-fetch": "^2.5.6",
    "apollo-server-express": "^2.12.0",
    "chalk": "^3.0.0",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "execa": "^4.0.0",
    "express": "^4.17.1",
    "fs-nextra": "^0.4.7",
    "fuse.js": "^3.6.1",
    "glob": "^7.1.6",
    "graphql-type-json": "^0.3.1",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "jest-circus": "^25.3.0",
    "lint-staged": "^10.1.6",
    "node-fetch": "^2.6.0",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.4",
    "reflect-metadata": "^0.1.13",
    "standard-version": "^7.1.0",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.8.2",
    "type-graphql": "^0.17.6",
    "typescript": "^3.8.3"
  },
  "files": [
    "dist/*",
    "!dist/*.tsbuildinfo"
  ],
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/favware/graphql-pokemon.git"
  },
  "bugs": {
    "url": "https://github.com/favware/graphql-pokemon/issues"
  },
  "keywords": [
    "favware",
    "typescript",
    "ts",
    "yarn",
    "graphql"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn scripts:validateNoOnly && yarn lint-staged",
      "post-merge": "yarn scripts:yarnAfterPull"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix --ext ts",
      "prettier --write"
    ],
    "*.json": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "resolutions": {
    "acorn": "^7.1.1",
    "minimist": "^1.2.2",
    "kind-of": "^6.0.3"
  }
}
