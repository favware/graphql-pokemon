{
  "name": "@favware/graphql-pokemon",
  "version": "5.3.0",
  "description": "Extensive Pokemon GraphQL API",
  "author": "@favware",
  "license": "MIT",
  "types": "generated/ts/graphql-pokemon.d.ts",
  "main": "generated/ts/graphql-pokemon.d.ts",
  "type": "module",
  "exports": {
    "import": "./generated/ts/graphql-pokemon.d.ts",
    "require": "./generated/ts/graphql-pokemon.d.ts"
  },
  "imports": {
    "#arguments/*": "./generated/api/arguments/*.js",
    "#assets/*": "./generated/api/assets/*.js",
    "#jsonAssets/*": "./generated/api/assets/*",
    "#dexdata/*": "./generated/api/assets/pokedex-data/*.js",
    "#resolvers/*": "./generated/api/resolvers/*.js",
    "#services/*": "./generated/api/services/*.js",
    "#structures/*": "./generated/api/structures/*.js",
    "#utils/*": "./generated/api/utils/*.js",
    "#root/*": "./generated/api/*.js",
    "#scripts/*": "./scripts/*.mjs"
  },
  "homepage": "https://graphqlpokemon.favware.tech/",
  "scripts": {
    "lint": "eslint src scripts __tests__ --ext ts,mjs --fix",
    "format": "prettier --write --loglevel=warn {src,scripts}/**/*.{ts,json}",
    "test": "jest",
    "start": "node --experimental-json-modules generated/api/index.js",
    "build": "tsc -b src",
    "watch": "tsc -b src -w",
    "clean": "node scripts/clean.mjs",
    "dev": "yarn build && yarn start",
    "commit": "git-cz",
    "cz": "git-cz",
    "update": "yarn upgrade-interactive --latest",
    "sversion": "standard-version",
    "codegen": "graphql-codegen",
    "postcodegen": "replace-in-file /}\\;/g } generated/ts/graphql-pokemon.d.ts --isRegex",
    "au:tiers": "node --experimental-json-modules scripts/smogontiers.mjs",
    "au:learnsets": "node --experimental-json-modules scripts/pokemonlearnsets.mjs",
    "prepare": "husky install .github/husky"
  },
  "dependencies": {
    "graphql": "^15.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@discordjs/collection": "^0.1.6",
    "@graphql-codegen/cli": "^1.21.4",
    "@graphql-codegen/schema-ast": "^1.18.2",
    "@graphql-codegen/typescript": "^1.22.0",
    "@graphql-codegen/typescript-resolvers": "^1.19.1",
    "@sapphire/eslint-config": "^3.1.2",
    "@sapphire/fetch": "^1.0.1",
    "@sapphire/prettier-config": "^1.1.1",
    "@sapphire/time-utilities": "^1.3.1",
    "@sapphire/ts-config": "^2.2.1",
    "@types/jest": "^26.0.23",
    "@types/koa": "^2.13.1",
    "@types/node": "^15.3.0",
    "@types/node-fetch": "^2.5.10",
    "apollo-server-koa": "^2.24.0",
    "class-validator": "^0.13.1",
    "colorette": "^1.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "fuse.js": "^6.4.6",
    "graphql-parse-resolve-info": "^4.11.0",
    "graphql-type-json": "^0.3.2",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "koa": "^2.13.1",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "pretty-quick": "^3.1.0",
    "reflect-metadata": "^0.1.13",
    "replace-in-file": "^6.2.0",
    "standard-version": "^9.3.0",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "type-graphql": "^1.1.1",
    "typescript": "^4.2.4",
    "utility-types": "^3.10.0"
  },
  "files": [
    "generated/ts/graphql-pokemon.d.ts",
    "generated/ts/graphql-pokemon.graphql"
  ],
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/favware/graphql-pokemon.git"
  },
  "bugs": {
    "url": "https://github.com/favware/graphql-pokemon/issues"
  },
  "keywords": [
    "favware",
    "typescript",
    "ts",
    "yarn",
    "graphql"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "resolutions": {
    "acorn": "^8.2.4",
    "minimist": "^1.2.5",
    "kind-of": "^6.0.3",
    "lodash": "^4.17.21",
    "dot-prop": "^6.0.1",
    "node-fetch": "^2.6.1"
  }
}
