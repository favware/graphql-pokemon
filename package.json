{
  "name": "@favware/graphql-pokemon",
  "version": "5.1.1",
  "description": "Extensive Pokemon GraphQL API",
  "author": "@favware",
  "license": "MIT",
  "types": "generated/ts/graphql-pokemon.d.ts",
  "main": "generated/ts/graphql-pokemon.d.ts",
  "exports": {
    "import": "./generated/ts/graphql-pokemon.d.ts",
    "require": "./generated/ts/graphql-pokemon.d.ts"
  },
  "homepage": "https://graphqlpokemon.favware.tech/",
  "scripts": {
    "lint": "eslint src scripts __tests__ --ext ts --fix",
    "format": "prettier --write --loglevel=warn {src,scripts}/**/*.{ts,json}",
    "test": "jest",
    "start": "node generated/api",
    "build": "tsc -b src",
    "watch": "tsc -b src -w",
    "clean": "tsc -b src --clean",
    "dev": "yarn build && yarn start",
    "commit": "git-cz",
    "cz": "git-cz",
    "update": "yarn upgrade-interactive --latest",
    "sversion": "standard-version",
    "codegen": "graphql-codegen",
    "postcodegen": "replace-in-file /}\\;/g } generated/ts/graphql-pokemon.d.ts --isRegex",
    "cloud:start": "node index.js",
    "scripts:runner": "ts-node -P scripts/tsconfig.json",
    "au:tiers": "yarn scripts:runner scripts/smogontiers",
    "au:learnsets": "yarn scripts:runner scripts/pokemonlearnsets",
    "prepublishOnly": "pinst --disable",
    "postinstall": "husky install .github/husky",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "graphql": "^15.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@discordjs/collection": "^0.1.6",
    "@graphql-codegen/cli": "^1.20.1",
    "@graphql-codegen/schema-ast": "^1.18.1",
    "@graphql-codegen/typescript": "^1.21.0",
    "@graphql-codegen/typescript-resolvers": "^1.18.2",
    "@sapphire/eslint-config": "^2.1.1",
    "@sapphire/prettier-config": "^1.0.6",
    "@sapphire/time-utilities": "^1.2.8",
    "@sapphire/ts-config": "^2.0.0",
    "@types/jest": "^26.0.20",
    "@types/koa": "^2.11.7",
    "@types/node": "^14.14.31",
    "@types/node-fetch": "^2.5.8",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "apollo-server-koa": "^2.21.0",
    "class-validator": "^0.13.1",
    "colorette": "^1.2.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "fs-nextra": "^0.5.1",
    "fuse.js": "^6.4.6",
    "graphql-parse-resolve-info": "^4.11.0",
    "graphql-type-json": "^0.3.2",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "koa": "^2.13.1",
    "lint-staged": "^10.5.4",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "pinst": "^2.1.4",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "reflect-metadata": "^0.1.13",
    "replace-in-file": "^6.2.0",
    "standard-version": "^9.1.0",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "type-graphql": "^1.1.1",
    "typescript": "^4.1.5",
    "utility-types": "^3.10.0"
  },
  "files": [
    "generated/ts/graphql-pokemon.d.ts",
    "generated/ts/graphql-pokemon.graphql"
  ],
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/favware/graphql-pokemon.git"
  },
  "bugs": {
    "url": "https://github.com/favware/graphql-pokemon/issues"
  },
  "keywords": [
    "favware",
    "typescript",
    "ts",
    "yarn",
    "graphql"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "resolutions": {
    "acorn": "^8.0.5",
    "minimist": "^1.2.5",
    "kind-of": "^6.0.3",
    "lodash": "^4.17.20",
    "dot-prop": "^6.0.1",
    "node-fetch": "^2.6.1"
  }
}
