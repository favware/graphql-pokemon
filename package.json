{
  "name": "@favware/graphql-pokemon",
  "version": "0.0.1",
  "description": "Expansive Pokemon GraphQL API",
  "author": "@favware",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://favware.tech/api",
  "scripts": {
    "lint": "eslint src scripts __tests__ --ext ts --fix",
    "test": "jest",
    "build": "tsc",
    "watch": "tsc -w",
    "dev": "nodemon dist/index.js",
    "commit": "git-cz",
    "cz": "git-cz",
    "up": "yarn upgrade-interactive --latest",
    "version": "standard-version",
    "codegen": "graphql-codegen",
    "cloud:start": "node index.js",
    "prepublishOnly": "yarn build && yarn codegen",
    "scripts:runner": "ts-node -P tsconfig.eslint.json",
    "scripts:validateNoOnly": "yarn scripts:runner scripts/validateNoOnly",
    "scripts:yarnAfterPull": "yarn scripts:runner scripts/yarnAfterPull",
    "scripts:formatsupdater": "yarn scripts:runner scripts/updateFormats"
  },
  "dependencies": {
    "@discordjs/collection": "^0.1.3",
    "apollo-server-express": "^2.9.7",
    "express": "^4.17.1",
    "fuse.js": "^3.4.5",
    "graphql": "^14.5.8",
    "graphql-type-json": "^0.3.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^0.17.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@favware/eslint-config": "^3.1.4",
    "@favware/ts-config": "^1.0.2",
    "@graphql-codegen/cli": "^1.8.3",
    "@graphql-codegen/flow": "^1.8.3",
    "@graphql-codegen/flow-resolvers": "^1.8.3",
    "@graphql-codegen/typescript": "^1.8.3",
    "@graphql-codegen/typescript-resolvers": "^1.8.3",
    "@klasa/timestamp": "^0.0.1",
    "@types/express": "^4.17.2",
    "@types/glob": "^7.1.1",
    "@types/graphql": "^14.5.0",
    "@types/graphql-type-json": "^0.3.2",
    "@types/node": "^12.12.7",
    "@types/node-fetch": "^2.5.3",
    "chalk": "^3.0.0",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.6.0",
    "execa": "^3.2.0",
    "fs-nextra": "^0.4.6",
    "glob": "^7.1.6",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "node-fetch": "^2.6.0",
    "nodemon": "^1.19.4",
    "standard-version": "^7.0.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.7.2"
  },
  "files": [
    "dist/index.d.ts",
    "dist/client.d.ts",
    "dist/client.flow.js"
  ],
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/favware/graphql-pokemon.git"
  },
  "bugs": {
    "url": "https://github.com/favware/graphql-pokemon/issues"
  },
  "keywords": [
    "favware",
    "typescript",
    "ts",
    "yarn",
    "graphql"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn scripts:validateNoOnly && yarn lint-staged",
      "post-merge": "yarn scripts:yarnAfterPull"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "yarn lint",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
