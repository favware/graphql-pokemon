{
  "name": "@favware/graphql-pokemon",
  "version": "7.3.4",
  "description": "Extensive Pokemon GraphQL API",
  "author": "@favware",
  "license": "MIT",
  "type": "module",
  "main": "dist/graphql-pokemon.cjs",
  "module": "dist/graphql-pokemon.js",
  "types": "dist/graphql-pokemon.d.cts",
  "exports": {
    "import": {
      "types": "./dist/graphql-pokemon.d.ts",
      "default": "./dist/graphql-pokemon.js"
    },
    "require": {
      "types": "./dist/graphql-pokemon.d.cts",
      "default": "./dist/graphql-pokemon.cjs"
    }
  },
  "sideEffects": false,
  "imports": {
    "#assets/*": "./api/lib/assets/*.js",
    "#jsonAssets/*": "./api/lib/assets/*.json",
    "#dexdata/*": "./api/lib/assets/pokedex-data/*.js",
    "#resolvers/*": "./api/lib/resolvers/*.js",
    "#types/*": "./api/lib/types/*.js",
    "#utils/*": "./api/lib/utils/*.js",
    "#validations/*": "./api/lib/validations/*.js",
    "#mappers/*": "./api/lib/mappers/*.js",
    "#root/*": "./api/*.js"
  },
  "homepage": "https://graphql-pokemon.js.org/",
  "scripts": {
    "lint": "eslint src tests scripts --ext ts --fix",
    "format": "prettier --write --log-level=warn \"{src,tests,scripts}/**/*.{ts,json}\"",
    "test": "vitest run",
    "typecheck": "tsc -b src && tsc -b tests",
    "start": "node --enable-source-maps api/index.js",
    "build": "tsup --onSuccess \"yarn tsx scripts/on-build-success.ts\"",
    "watch": "tsup --watch --onSuccess \"yarn tsx scripts/on-build-success.ts\"",
    "dev": "yarn build && yarn start",
    "clean": "yarn tsx scripts/clean.ts",
    "bump": "cliff-jumper",
    "check-update": "cliff-jumper --dry-run",
    "codegen": "graphql-codegen && npm-run-all -s \"typescript:*\"",
    "typescript:replace-invalid-code": "replace-in-file /\\}\\;/g \"}\" codegen/graphql-pokemon.ts --isRegex",
    "typescript:compile": "tsup --config tsup.config-codegen.ts",
    "au:tiers": "yarn tsx ./scripts/smogontiers.ts",
    "au:learnsets": "node ./scripts/pokemonlearnsets.ts",
    "au:testdata": "yarn tsx scripts/update-test-files.ts",
    "au:flavors": "yarn tsx ./scripts/data-gen-scripts/flavor-text-updater/flavor-text-updater.ts",
    "tsx": "tsx --experimental-network-imports --tsconfig ./scripts/data-gen-scripts/tsconfig.json"
  },
  "dependencies": {
    "graphql": "^16.8.1"
  },
  "devDependencies": {
    "@apollo/server": "^4.9.5",
    "@as-integrations/koa": "^1.1.1",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@discordjs/collection": "^2.0.0",
    "@favware/cliff-jumper": "^2.2.3",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/schema-ast": "^4.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@koa/cors": "^4.0.0",
    "@sapphire/eslint-config": "^5.0.2",
    "@sapphire/fetch": "^2.4.1",
    "@sapphire/prettier-config": "^2.0.0",
    "@sapphire/shapeshift": "^3.9.3",
    "@sapphire/timestamp": "^1.0.1",
    "@sapphire/ts-config": "^5.0.0",
    "@sapphire/utilities": "^3.13.0",
    "@skyra/jaro-winkler": "^1.1.0",
    "@swc/core": "^1.3.99",
    "@types/async": "^3.2.24",
    "@types/cheerio": "^0.22.35",
    "@types/koa": "^2.13.12",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa__cors": "^4.0.3",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^0.34.6",
    "async": "^3.2.5",
    "colorette": "^2.0.20",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "graphql-parse-resolve-info": "^4.14.0",
    "graphql-tag": "^2.12.6",
    "koa": "^2.14.2",
    "koa-bodyparser": "^4.4.1",
    "lint-staged": "^15.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "replace-in-file": "^7.0.2",
    "tsup": "^8.0.1",
    "tsx": "^4.5.0",
    "typescript": "^5.3.2",
    "vitest": "^0.34.6"
  },
  "resolutions": {
    "ansi-regex": "^5.0.1",
    "minimist": "^1.2.8"
  },
  "files": [
    "dist/*",
    "graphql/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/favware/graphql-pokemon.git"
  },
  "bugs": {
    "url": "https://github.com/favware/graphql-pokemon/issues"
  },
  "keywords": [
    "favware",
    "typescript",
    "ts",
    "yarn",
    "graphql"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write",
    "*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "yarn@4.0.2",
  "volta": {
    "node": "20.9.0"
  }
}
