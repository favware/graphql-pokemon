{
  "name": "@favware/graphql-pokemon",
  "version": "1.7.0",
  "description": "Expansive Pokemon GraphQL API",
  "author": "@favware",
  "license": "MIT",
  "types": "dist/graphql-pokemon.d.ts",
  "main": "dist/graphql-pokemon.js",
  "homepage": "https://favware.tech/api",
  "scripts": {
    "lint": "eslint src scripts __tests__ --ext ts --fix",
    "style": "yarn pretty-quick",
    "test": "jest",
    "build": "tsc -b src",
    "watch": "tsc -b src -w",
    "dev": "nodemon build/index.js",
    "commit": "git-cz",
    "cz": "git-cz",
    "up": "yarn upgrade-interactive --latest",
    "sversion": "standard-version",
    "codegen": "graphql-codegen && tsc -b typings",
    "cloud:start": "node index.js",
    "prepublishOnly": "yarn codegen",
    "scripts:runner": "ts-node -P tsconfig.eslint.json",
    "scripts:validateNoOnly": "yarn scripts:runner scripts/validateNoOnly",
    "scripts:yarnAfterPull": "yarn scripts:runner scripts/yarnAfterPull",
    "scripts:smogontiersupdater": "yarn scripts:runner scripts/updateSmogonTiers",
    "scripts:updatelearnsets": "yarn scripts:runner scripts/UpdateLearnsets"
  },
  "dependencies": {
    "graphql": "^14.5.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.4",
    "@commitlint/config-conventional": "^8.3.4",
    "@discordjs/collection": "^0.1.3",
    "@favware/eslint-config": "^3.1.6",
    "@favware/ts-config": "^1.1.1",
    "@graphql-codegen/cli": "^1.11.2",
    "@graphql-codegen/typescript": "^1.11.2",
    "@graphql-codegen/typescript-resolvers": "^1.11.2",
    "@klasa/timestamp": "^0.0.1",
    "@types/express": "^4.17.2",
    "@types/glob": "^7.1.1",
    "@types/graphql": "^14.5.0",
    "@types/graphql-type-json": "^0.3.2",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.6",
    "@types/node-fetch": "^2.5.4",
    "apollo-server-express": "^2.9.16",
    "chalk": "^3.0.0",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.8.0",
    "execa": "^4.0.0",
    "express": "^4.17.1",
    "fs-nextra": "^0.4.7",
    "fuse.js": "^3.4.6",
    "glob": "^7.1.6",
    "graphql-type-json": "^0.3.1",
    "husky": "^4.0.6",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "lint-staged": "^9.5.0",
    "node-fetch": "^2.6.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "reflect-metadata": "^0.1.13",
    "standard-version": "^7.0.1",
    "ts-jest": "^24.3.0",
    "ts-node": "^8.6.0",
    "type-graphql": "^0.17.6",
    "typescript": "^3.7.4"
  },
  "files": ["dist/*", "!dist/*.tsbuildinfo"],
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/favware/graphql-pokemon.git"
  },
  "bugs": {
    "url": "https://github.com/favware/graphql-pokemon/issues"
  },
  "keywords": ["favware", "typescript", "ts", "yarn", "graphql"],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn scripts:validateNoOnly && pretty-quick --staged && yarn lint-staged",
      "post-merge": "yarn scripts:yarnAfterPull"
    }
  },
  "commitlint": {
    "extends": ["@commitlint/config-conventional"]
  },
  "lint-staged": {
    "*.{js,ts,tsx}": ["yarn lint", "git add"]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
