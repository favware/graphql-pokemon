{
  "name": "@favware/graphql-pokemon",
  "version": "6.2.8",
  "description": "Extensive Pokemon GraphQL API",
  "author": "@favware",
  "license": "MIT",
  "type": "module",
  "main": "generated/ts/graphql-pokemon.cjs",
  "module": "generated/ts/graphql-pokemon.mjs",
  "types": "generated/ts/graphql-pokemon.d.ts",
  "exports": {
    "import": "./generated/ts/graphql-pokemon.mjs",
    "require": "./generated/ts/graphql-pokemon.cjs"
  },
  "sideEffects": false,
  "imports": {
    "#arguments/*": "./generated/api/arguments/*.js",
    "#assets/*": "./generated/api/assets/*.js",
    "#jsonAssets/*": "./generated/api/assets/*",
    "#dexdata/*": "./generated/api/assets/pokedex-data/*.js",
    "#resolvers/*": "./generated/api/resolvers/*.js",
    "#services/*": "./generated/api/services/*.js",
    "#structures/*": "./generated/api/structures/*.js",
    "#utils/*": "./generated/api/utils/*.js",
    "#root/*": "./generated/api/*.js",
    "#scripts/*": "./scripts/*.mjs"
  },
  "homepage": "https://graphqlpokemon.favware.tech/",
  "scripts": {
    "lint": "eslint src scripts __tests__ --ext ts,mjs --fix",
    "format": "prettier --write --loglevel=warn {src,scripts}/**/*.{ts,json}",
    "test": "jest",
    "start": "node --enable-source-maps --experimental-json-modules generated/api/index.js",
    "build": "tsc -b src",
    "watch": "tsc -b src -w",
    "clean": "node scripts/clean.mjs",
    "dev": "yarn build && yarn start",
    "commit": "git-cz",
    "cz": "git-cz",
    "update": "yarn upgrade-interactive",
    "sversion": "standard-version",
    "codegen": "node scripts/clean-generated.mjs && graphql-codegen && npm-run-all -s \"typescript:*\"",
    "typescript:replace-invalid-code": "replace-in-file /\\}\\;/g \"}\" generated/ts/graphql-pokemon.ts --isRegex",
    "typescript:use-type-import": "replace-in-file /import/g \"import type\" generated/ts/graphql-pokemon.ts --isRegex",
    "typescript:compile": "rollup -c scripts/rollup.config.mjs",
    "typescript:cleanup": "node scripts/postcodegen.mjs",
    "au:tiers": "node --experimental-json-modules scripts/smogontiers.mjs",
    "au:learnsets": "node --experimental-json-modules scripts/pokemonlearnsets.mjs",
    "prepare": "husky install .github/husky"
  },
  "dependencies": {
    "graphql": "^15.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@discordjs/collection": "^0.4.0",
    "@graphql-codegen/cli": "^2.3.1",
    "@graphql-codegen/schema-ast": "^2.4.1",
    "@graphql-codegen/typescript": "^2.4.2",
    "@graphql-codegen/typescript-resolvers": "^2.4.3",
    "@sapphire/eslint-config": "^4.0.8",
    "@sapphire/fetch": "^2.0.3",
    "@sapphire/prettier-config": "^1.2.7",
    "@sapphire/time-utilities": "^1.5.0",
    "@sapphire/ts-config": "^3.1.6",
    "@types/jest": "^27.4.0",
    "@types/koa": "^2.13.4",
    "@types/node": "^16.11.17",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "apollo-server-koa": "^3.6.1",
    "class-validator": "^0.13.2",
    "colorette": "^2.0.16",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fuse.js": "^6.5.3",
    "graphql-parse-resolve-info": "^4.12.0",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "jest-circus": "^27.4.5",
    "koa": "^2.13.4",
    "lint-staged": "^12.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "reflect-metadata": "^0.1.13",
    "replace-in-file": "^6.3.2",
    "rollup": "^2.62.0",
    "rollup-plugin-typescript2": "^0.31.1",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "type-graphql": "^1.1.1",
    "typescript": "^4.5.4",
    "utility-types": "^3.10.0"
  },
  "files": [
    "generated/ts"
  ],
  "engines": {
    "node": ">=16",
    "npm": ">=7.24.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/favware/graphql-pokemon.git"
  },
  "bugs": {
    "url": "https://github.com/favware/graphql-pokemon/issues"
  },
  "keywords": [
    "favware",
    "typescript",
    "ts",
    "yarn",
    "graphql"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "yarn@3.1.1"
}
